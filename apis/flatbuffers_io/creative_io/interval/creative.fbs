namespace Creative;

enum Clarity : byte{ LD, SD, HD }


table CreativeId {
    id:int64;
    clarity:Clarity;
}

table CreativeAttr {
    id:int64;
    rule:CreativeRule;
}

table CreativeRule {
    adtype:string;
}

table Cmd {
    cmd:int64;
}


rpc_service CreativeStorage {
  Store(CreativeAttr):Cmd;
  Retrieve(CreativeId):CreativeAttr;
}